{
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "supabase": "^2.30.4"
  },
  "scripts": {
    "test": "npm test --workspace backend",
    "lint:frontend": "npm run lint --workspace frontend",
    "format:frontend": "npm run format --workspace frontend",
    "lint:backend": "eslint \"api/**/*.ts\" \"backend/**/*.ts\" --fix",
    "format:backend": "prettier --write \"api/**/*.ts\" \"backend/**/*.ts\"",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "format": "npm run format:frontend && npm run format:backend",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "build": "run-p build:frontend build:backend docs:build",
    "build:frontend": "npm run build --workspace frontend",
    "build:backend": "npm run build --workspace backend"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "overrides": {
    "esbuild": "^0.25.0",
    "path-to-regexp": "^8.2.0",
    "undici": "^7.11.0"
  },
  "dependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.46.2",
    "lightningcss": "^1.30.1"
  }
}
